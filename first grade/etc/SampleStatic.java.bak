class SampleStatic 
{
	public static int a;
	public int b;
	public int c;

	public static void main(String[] args) 
	{
		SampleStatic s1=new SampleStatic();
		SampleStatic s2=new SampleStatic();

	    s1.a=50;
		s1.b=200;
		s1.c=300;
		s2.a=400;
		s2.b=500;
		s2.c=600;

		System.out.println("a="+s1.a+"b="+s1.b+",c="+s1.c);
		System.out.println("a="+s2.a+"b="+s2.b+",c="+s2.c);

	}
}

